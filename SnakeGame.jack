class SnakeGame {

	field Snake snake;
	field int exitButton; //'q' button

	constructor SnakeGame new() {
		let snake = Snake.new(5, 10);
		let exitButton = 81;
		return this;
	} 

	method void start() {
		var int currentKeyPressed;
		while (~(currentKeyPressed = exitButton)) {
			let currentKeyPressed = Keyboard.keyPressed();
		}

		
		return;
	}

	method void dispose() {
		do snake.dispose();
		do Memory.deAlloc(this);
		return;
	}
}